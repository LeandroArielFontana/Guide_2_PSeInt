Algoritmo Ejercicio_4_Guia_2
	Definir value Como Caracter;
	
	Escribir "Digite S o N"
	Leer value;
	
	Segun value hacer
		"S":
			Escribir "Correcto";
		"N":
			Escribir "Incorrecto";
		"s":
			Escribir "Correcto";
		"n":
			Escribir "Incorrecto";
		De Otro Modo:
			Escribir "Opcion no valida"
	FinSegun
FinAlgoritmo
